{
  "name": "webpack-js-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project to React app",
  "main": "main.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "format-js": "prettier {config,src}/**/*.js --write",
    "format-scss": "stylelint src/**/*.scss --fix",
    "lint": "eslint . --fix",
    "clean": "npm run format-js && npm run format-scss && npm run lint",
    "translate": "extract-messages --locales=en,vi --output \"src/languages\" \"src/**/*.messages.js\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-js-boilerplate/webpack-js-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webpack-js-boilerplate/webpack-js-boilerplate/issues"
  },
  "homepage": "https://github.com/webpack-js-boilerplate/webpack-js-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "autoprefixer": "^10.3.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-intl-auto": "^3.3.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "bundle-stats-webpack-plugin": "^3.1.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^8.0.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-react-intl-messages": "^4.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "postcss": "^8.3.8",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "antd": "^4.18.7",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "fontawesome": "^5.6.3",
    "fortawesome": "0.0.1-security",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.2",
    "react-inlinesvg": "^2.3.0",
    "react-router-dom": "^6.2.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ]
  }
}
